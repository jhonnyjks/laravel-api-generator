{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Client Server MVP",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "convertio/convertio-php": "^0.4.0",
        "darkaonline/l5-swagger": "^8.6",
        "directorytree/ldaprecord-laravel": "^2.7",
        "doctrine/dbal": "~2.10",
        "fakerphp/faker": "^1.21",
        "flowjs/flow-php-server": "^1.0",
        "ilovepdf/ilovepdf-php": "^1.1",
        "infyomlabs/laravel-generator": "^6.0",
        "infyomlabs/laravel-ui-adminlte": "^5.0",
        "infyomlabs/swagger-generator": "^2.0",
        "intervention/image": "^3.11",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.5",
        "laravel/tinker": "^2.10",
        "laravelcollective/html": "^6.4",
        "maatwebsite/excel": "^3.1",
        "phpoffice/phpword": "^1.4",
        "prettus/l5-repository": "^2.6",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.6",
        "thiagoalessio/tesseract_ocr": "^2.13"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "infyomlabs/laravel-generator": "^6.0",
        "kitloong/laravel-migrations-generator": "^6.10",
        "laracasts/generators": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
